{
    "name": "58shuwei/sample-docker-plugin",
    "description": "Docker Sample Plugin",
    "type": "shuwei-platform-plugin",
    "license": "MIT",
    "extra": {
        "shuwei-plugin-class": "DockerSamplePlugin\\DockerSamplePlugin",
        "author": "58shuwei",
        "label": {
            "de-DE": "Dockware Sample Plugin",
            "en-GB": "Dockware Sample Plugin"
        }
    },
    "require": {
        "php": ">= 7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.3.4",
        "phpstan/phpstan": "^0.12.18",
        "shuwei/core": "*",
        "shuwei/storefront": "*"
    },
    "autoload": {
        "psr-4": {
            "DockerSamplePlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DockerSamplePlugin\\Tests\\": "tests/"
        }
    }
}
